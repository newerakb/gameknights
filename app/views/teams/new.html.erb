<h1>New Team</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@team) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :name, "Team Name" %>
      <%= f.text_field :name %>

      <%= f.submit "Create new team", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
